CREATE CACHED TABLE FR_T_JROLEENTRYPRIVILEGE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ROLEID BIGINT NOT NULL,TYPE INTEGER NOT NULL,ENTRYID BIGINT NOT NULL,CONSTRAINT SYS_CT_80 UNIQUE(ROLEID,TYPE,ENTRYID),CONSTRAINT FR_FK_JROLEENTRYPRIVILEGE__ROLEID FOREIGN KEY(ROLEID) REFERENCES FR_T_JROLE(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_SROLEENTRYPRIVILEGE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ROLEID BIGINT NOT NULL,TYPE INTEGER NOT NULL,ENTRYID BIGINT NOT NULL,CONSTRAINT SYS_CT_84 UNIQUE(ROLEID,TYPE,ENTRYID),CONSTRAINT FR_FK_SROLEENTRYPRIVILEGE__ROLEID FOREIGN KEY(ROLEID) REFERENCES FR_T_SROLE(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_CHARTLET(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(63) NOT NULL,SOURCEPATH VARCHAR(255) NOT NULL,PARAMS VARCHAR(1023),CONSTRAINT SYS_CT_88 UNIQUE(NAME))
CREATE CACHED TABLE FR_T_USERWIDGETLET(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERID BIGINT NOT NULL,TYPE INTEGER NOT NULL,LETID BIGINT NOT NULL,CONSTRAINT FR_FK_USERWIDGETLET__USERID FOREIGN KEY(USERID) REFERENCES FR_T_USER(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_FAVORITENODE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERID BIGINT NOT NULL,TYPE INTEGER NOT NULL,ENTRYID BIGINT NOT NULL,CONSTRAINT FR_FK_FAVORITENODE__USERID FOREIGN KEY(USERID) REFERENCES FR_T_USER(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_NOTICE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CONTENT VARCHAR(1023),TIME DATE)
CREATE CACHED TABLE FR_T_CHARTLET_JROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,JROLEID BIGINT NOT NULL,CHARTLETID BIGINT NOT NULL,CONSTRAINT SYS_CT_98 UNIQUE(JROLEID,CHARTLETID),CONSTRAINT FR_FK_CHARTLET_JROLE__JROLEID FOREIGN KEY(JROLEID) REFERENCES FR_T_JROLE(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_CHARTLET_JROLE__CHARTLETID FOREIGN KEY(CHARTLETID) REFERENCES FR_T_CHARTLET(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_SROLE_USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERID BIGINT NOT NULL,SROLEID BIGINT NOT NULL,CONSTRAINT SYS_CT_102 UNIQUE(USERID,SROLEID),CONSTRAINT FR_FK_SROLE_USER__USERID FOREIGN KEY(USERID) REFERENCES FR_T_USER(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_SROLE_USER__SROLEID FOREIGN KEY(SROLEID) REFERENCES FR_T_SROLE(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_DEPARTMENT_POST_USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERID BIGINT NOT NULL,DEPARTMENTID BIGINT NOT NULL,POSTID BIGINT NOT NULL,CONSTRAINT SYS_CT_106 UNIQUE(USERID,DEPARTMENTID,POSTID),CONSTRAINT FR_FK_DEPARTMENT_POST_USER__USERID FOREIGN KEY(USERID) REFERENCES FR_T_USER(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_DEPARTMENT_POST_USER__DEPARTMENTID FOREIGN KEY(DEPARTMENTID) REFERENCES FR_T_DEPARTMENT(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_DEPARTMENT_POST_USER__POSTID FOREIGN KEY(POSTID) REFERENCES FR_T_POST(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_CHARTLET_SROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,SROLEID BIGINT NOT NULL,CHARTLETID BIGINT NOT NULL,CONSTRAINT SYS_CT_110 UNIQUE(SROLEID,CHARTLETID),CONSTRAINT FR_FK_CHARTLET_SROLE__SROLEID FOREIGN KEY(SROLEID) REFERENCES FR_T_SROLE(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_CHARTLET_SROLE__CHARTLETID FOREIGN KEY(CHARTLETID) REFERENCES FR_T_CHARTLET(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_JROLE_MODULE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,JROLEID BIGINT NOT NULL,MODULEID BIGINT NOT NULL,CONSTRAINT SYS_CT_114 UNIQUE(JROLEID,MODULEID),CONSTRAINT FR_FK_JROLE_MODULE__JROLEID FOREIGN KEY(JROLEID) REFERENCES FR_T_JROLE(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_JROLE_MODULE__MODULEID FOREIGN KEY(MODULEID) REFERENCES FR_T_MODULE(ID) ON DELETE CASCADE)
CREATE CACHED TABLE FR_T_MODULE_SROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,SROLEID BIGINT NOT NULL,MODULEID BIGINT NOT NULL,CONSTRAINT SYS_CT_118 UNIQUE(SROLEID,MODULEID),CONSTRAINT FR_FK_MODULE_SROLE__SROLEID FOREIGN KEY(SROLEID) REFERENCES FR_T_SROLE(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_MODULE_SROLE__MODULEID FOREIGN KEY(MODULEID) REFERENCES FR_T_MODULE(ID) ON DELETE CASCADE)
CREATE CACHED TABLE QRTZ_JOB_DETAILS(JOB_NAME VARCHAR(200) NOT NULL,JOB_GROUP VARCHAR(200) NOT NULL,DESCRIPTION VARCHAR(250),JOB_CLASS_NAME VARCHAR(250) NOT NULL,IS_DURABLE BOOLEAN NOT NULL,IS_VOLATILE BOOLEAN NOT NULL,IS_STATEFUL BOOLEAN NOT NULL,REQUESTS_RECOVERY BOOLEAN NOT NULL,JOB_DATA BINARY,PRIMARY KEY(JOB_NAME,JOB_GROUP))
