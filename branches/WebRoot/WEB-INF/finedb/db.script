CREATE CACHED TABLE FR_T_USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,USERNAME VARCHAR(20) NOT NULL,PASSWORD VARCHAR(20) NOT NULL,REALNAME VARCHAR(20) NOT NULL,BIRTHDAY DATE,MALE BOOLEAN,MOBILE VARCHAR(20),WORKPHONE VARCHAR(20),EMAIL VARCHAR(127),CONSTRAINT SYS_CT_46 UNIQUE(USERNAME))
CREATE CACHED TABLE FR_T_SROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,ROLENAME VARCHAR(63) NOT NULL,DESCRIPTION VARCHAR(255),CONSTRAINT SYS_CT_50 UNIQUE(ROLENAME))
CREATE CACHED TABLE FR_T_POST(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,POSTNAME VARCHAR(63) NOT NULL,DESCRIPTION VARCHAR(255),CONSTRAINT SYS_CT_54 UNIQUE(POSTNAME))
CREATE CACHED TABLE FR_T_MODULE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PARENTID BIGINT NOT NULL,NAME VARCHAR(63) NOT NULL,ACTION VARCHAR(255),DESCRIPTION VARCHAR(255),CONSTRAINT SYS_CT_58 UNIQUE(NAME))
CREATE CACHED TABLE FR_T_DEPARTMENT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PID BIGINT NOT NULL,NAME VARCHAR(63) NOT NULL,DESCRIPTION VARCHAR(255),CONSTRAINT SYS_CT_62 UNIQUE(NAME))
CREATE CACHED TABLE FR_T_JROLE(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,POSTID BIGINT NOT NULL,DEPARTMENTID BIGINT NOT NULL,DESCRIPTION VARCHAR(255),CONSTRAINT SYS_CT_66 UNIQUE(POSTID,DEPARTMENTID),CONSTRAINT FR_FK_JROLE__POSTID FOREIGN KEY(POSTID) REFERENCES FR_T_POST(ID) ON DELETE CASCADE,CONSTRAINT FR_FK_JROLE__DEPARTMENTID FOREIGN KEY(DEPARTMENTID) REFERENCES FR_T_DEPARTMENT(ID) ON DELETE CASCADE)
