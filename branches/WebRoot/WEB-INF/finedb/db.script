CREATE CACHED TABLE FR_REPORTLETENTRY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PARENT INTEGER NOT NULL,NAME VARCHAR(255) NOT NULL,REPORTLETPATH VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255),ISVIEW INTEGER NOT NULL,TRANSMITPARAMETERS VARCHAR(1023))
CREATE CACHED TABLE FR_FILEENTRY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PARENT INTEGER NOT NULL,NAME VARCHAR(255) NOT NULL,URI VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255))
CREATE CACHED TABLE FR_URLENTRY(ID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,PARENT INTEGER NOT NULL,NAME VARCHAR(255) NOT NULL,URL VARCHAR(255) NOT NULL,DESCRIPTION VARCHAR(255))
