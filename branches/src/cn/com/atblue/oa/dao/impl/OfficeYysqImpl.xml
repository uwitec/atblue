    <insert id="addOfficeYysq" parameterClass="OfficeYysq">
        <![CDATA[
            insert into office_yysq(
                sqid,yymc,yybm,yysy,bz,
				sqsj,yykssj,yyjssj,sqzt,process_id,
				connect_id,yyr,fs
            ) values(
                #sqid#,#yymc#,#yybm#,#yysy#,#bz#,
				#sqsj#,#yykssj#,#yyjssj#,#sqzt#,#processId#,
				#connectId#,#yyr#,#fs#
            )
        ]]>
